{
  "name": "dicionario-rasquido",
  "version": "1.0.1",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "compilerOptions": {
    "module": "CommonJS"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:csr": "ts-node scripts/build-csr",
    "build:ssr": "ts-node scripts/build-ssr",
    "postbuild": "npm run build:csr && npm run build:ssr",
    "start": "npm run prod",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "preload": "ts-node ./server/preload-vocab.ts",
    "lint": "eslint \"{server,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"server/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "agenda": "^4.3.0",
    "cache-manager": "^5.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "floating-vue": "^2.0.0-beta.20",
    "got": "^11.8.5",
    "mongodb": "^4.10.0",
    "nest": "^0.1.6",
    "node-html-parser": "^6.1.0",
    "nodemon": "^2.0.20",
    "pinia": "^2.0.9",
    "primevue": "^3.17.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "vite": "^2.7.12",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "workerpool": "^6.2.1",
    "youtube-transcript": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^29.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vue-cli-plugin-vuetify": "~2.5.8"
  }
}
